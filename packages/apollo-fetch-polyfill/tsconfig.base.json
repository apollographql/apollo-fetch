{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "declaration": true,
    "rootDir": "src",
    "outDir": "dist",
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "skipLibCheck": true
  }
}
